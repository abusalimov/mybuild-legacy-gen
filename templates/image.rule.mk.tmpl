__image_mk_file := $(MKGEN_DIR)/image.rule.mk

{% for stage in [1, 2] %}
__image_ld_scripts{{stage}} := \
    {{ files | file_path('%.lds.S', file_out.lds) | mk_list }}

__image_ld_objs{{stage}} := \
    {{ ld_modules | filterattrs('Build.stage', stage, default=1) |
        module_path(module_out.ld) | mk_list }}

__image_ld_libs{{stage}} := \
    {{ ar_modules | filterattrs('Build.stage', stage, default=1) |
        module_path(module_out.ar) | mk_list }}

{% endfor %}
